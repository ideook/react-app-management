{
  "name": "management",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "setup:https": "openssl genrsa -out ./cert/localhost.key 2048 && openssl req -new -x509 -key ./cert/localhost.key -out ./cert/localhost.crt -days 3650 -subj /CN=localhost"
  },
  "keywords": [
    "node js",
    "sql server",
    "mssql",
    "crud",
    "express",
    "sequelize",
    "rest api"
  ],
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "install": "^0.13.0",
    "mssql": "^8.1.0",
    "multer": "^1.4.4",
    "openssl": "^2.0.0",
    "sequelize": "^6.19.0",
    "tedious": "^14.4.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
